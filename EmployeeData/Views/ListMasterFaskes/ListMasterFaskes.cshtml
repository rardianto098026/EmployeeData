                                                                <div class="hidden-sm hidden-xs action-buttons">
                                                                    <a class="green" target="_blank" href="@Url.Action("EditFaskes", "ListMasterFaskes", new {idx = @Model.GetFaskes[i].idx,Faskes = @Model.GetFaskes[i].NamaFaskes_ID, Prov = @Model.GetFaskes[i].Provinsi_ID, Kota = @Model.GetFaskes[i].KotaMadya_ID, Kec = @Model.GetFaskes[i].Kecamatan_ID, TypeFaskes = Model.Faskes_GIGI_ID })">
                                                                        Edit
                                                                    </a>

                                                                    <a class="red" href="#">
                                                                        @Html.ActionLink("Delete", "DeleteFaskes", new { Faskes = @Model.GetFaskes[i].NamaFaskes_ID,Prov = @Model.GetFaskes[i].Provinsi_ID,Kota = @Model.GetFaskes[i].KotaMadya_ID, Kec = @Model.GetFaskes[i].Kecamatan_ID, TypeFaskes = Model.Faskes_GIGI_ID }, new { @onclick = "return confirm('Are you sure you want to delete this record?');" })
                                                                    </a>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                    }
                                                </tbody>
                                            </table>
                                        </div>
