       $("#Kotamadya_FASKES_Spouse").change(function () {
            $("#Kecamatan_FASKES_Spouse").empty();
            $("#FASKES_TK_Spouse").empty();
            $.ajax({
                type: 'POST',
                url: 'getKecamatanList_FASKES_Spouse',
                dataType: 'json',
                data: { id: $("#Kotamadya_FASKES_Spouse").val(), idx: $("#Provinsi_FASKES_Spouse").val() },

                success: function (user) {
                    $("#Kecamatan_FASKES_Spouse").empty();
                    $.each(user, function (i, usr) {
                        $("#Kecamatan_FASKES_Spouse").append('<option value="' + usr.Value + '">' +
                             usr.Text + '</option>');
                    });
                },
                error: function (ex) {
                    alert('Failed to retrieve states.' + ex);
                }
            });
            return false;
        })
    });
</script>
