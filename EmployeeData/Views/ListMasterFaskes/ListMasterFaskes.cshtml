<script src="~/assets/js/jquery-2.1.4.min.js"></script>

<script type="text/javascript">
    $(document).ready(function () {

        $("#Faskes_GIGI").change(function () {
            
            $("#Provinsi").empty();
            $("#KotaMadya").empty();
            $("#Kecamatan_FASKES").empty();
            $("#NamaFaskes").empty();
            $.ajax({
                type: 'POST',
                url: 'GetListMasterFaskes',
                dataType: 'json',
                data: { id: $("#Faskes_GIGI").val() },

                success: function (user) {
                    $("#Provinsi").empty();
                    $.each(user, function (i, usr) {
                        $("#Provinsi").append('<option value="' + usr.Value + '">' +
                             usr.Text + '</option>');
                    });
                },
                error: function (ex) {
                    alert('Failed to retrieve states.' + ex);
                }
            });
            return false;
        });
    });
</script>
