<script type="text/javascript">
    $(document).ready(function () {

        $("#StateID").change(function () {
            $("#Kotamadya").empty();
            $("#Kecamatan").empty();
            $("#Kelurahan").empty();
            $.ajax({
                type: 'POST',
                url: 'getKotaMadyaList',
                dataType: 'json',
                data: { id: $("#StateID").val() },

                success: function (user) {
                    $("#Kotamadya").empty();
                    $.each(user, function (i, usr) {
                        $("#Kotamadya").append('<option value="' + usr.Value + '">' +
                             usr.Text + '</option>');
                    });
                },
                error: function (ex) {
                    alert('Failed to retrieve states.' + ex);
                }
            });
            return false;
        });
    });
</script>
